  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:vue"],
      "runtimeExecutable": "npm",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "runtimeVersion": "20.18.1",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Build Vite-Vue3",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "build"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "cwd": "${workspaceFolder}/examples/vite-vue3",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
